{
  "multipart": [
    {
      "apply": {
        "model": "duckmod:block/egg_incubator"
      }
    },
    {
      "apply": {
        "model": "duckmod:block/incubated_egg"
      },
      "when": { "OR": [
        { "egg": "chicken" },
        { "egg": "duck" },
        { "egg": "emblem" }
      ]}
    }
  ]
}